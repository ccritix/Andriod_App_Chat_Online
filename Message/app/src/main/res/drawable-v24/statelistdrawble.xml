<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <gradient
                android:angle="270"
                android:endColor="#514A48"
                android:startColor="#514A48"/>
            <stroke
                android:width="1dp"
                android:color="#514A48"/>
            <corners android:radius="5dp"/>
            <padding
                android:bottom="10dp"
                android:left="10dp"
                android:right="10dp"
                android:top="10dp"/>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <gradient
                android:angle="270"
                android:endColor="#AFB42B"
                android:startColor="#AFB42B" />
            <stroke
                android:color="#AFB42B"
                android:width="1dp" />
            <corners android:radius="5dp" />
            <padding
                android:bottom="10dp"
                android:left="10dp"
                android:right="10dp"
                android:top="10dp" />
        </shape>
    </item>
</selector>